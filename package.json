{
  "name": "color-pen",
  "type": "module",
  "version": "0.0.1",
  "main": "cjs/index.cjs",
  "typings": "types/index.d.ts",
  "author": "earthnut  <earthnut.dev@outlook.com>  (https://earthnut.dev)",
  "description": "a color pen for supports colors terminal",
  "scripts": {
    "b": "rollup --config rollup.config.js && tsc -p tsconfig.types.json",
    "build": "npx ixxx rm dist run b",
    "push:version": "bash -c 'git add . && git commit -m \"version: ${1+ }$1 $(date +\"%Y-%m-%d %H:%M:%S\")${2:+ }$2\" && git push origin main' -- ",
    "push:submit": "bash -c 'git add . && git commit -m \"submit: $(date +\"%Y-%m-%d %H:%M:%S\")${1:+ }$1\" && git push origin main' -- ",
    "diff": "npx ixxx pkg diff",
    "eslint": "npx ixxx cls && eslint src",
    "beautify": "npm run prettier",
    "prettier": "npx ixxx cls && prettier . --write",
    "test": "jest --coverage",
    "report": "cat ./coverage/lcov.info | coveralls",
    "coverage": "codecov",
    "preview": "node example/index.js",
    "versionPatch": "npm version patch --force --no-git-tag-version --allow-same-version=true",
    "pub": "npm run build && cd dist && npm publish --access public && cd ../"
  },
  "files": [
    "mjs",
    "cjs",
    "types"
  ],
  "exports": {
    ".": {
      "import": {
        "default": "./mjs/index.mjs",
        "types": "./types/index.d.ts"
      },
      "require": {
        "default": "./cjs/index.cjs",
        "types": "./types/index.d.ts"
      }
    }
  },
  "keywords": [
    "color-pen"
  ],
  "license": "ISC",
  "homepage": "https://earthnut.dev/color-pen",
  "bugs": {
    "url": "https://github.com/earthnutDev/color-pen/issues",
    "email": "earthnut.dev@outlook.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/earthnutDev/color-pen.git')"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "a-type-of-js": ">=0.0.7",
    "a-js-tools": "^0.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.14.13",
    "codecov": "^3.8.2",
    "coveralls": "^3.1.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.9.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "lint-staged": "^15.5.0",
    "prettier": "^3.3.3",
    "rollup": "^4.19.2",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-copy": "^3.5.0",
    "ts-jest": "^29.2.6",
    "typescript": "^5.5.4",
    "typescript-eslint": "^7.18.0"
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx}": "eslint --fix",
    "*.{md,json,yml,yaml}": "prettier --write"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Edge versions",
    "not Safari >= 0",
    "not IE >= 0",
    "not Firefox >= 0"
  ],
  "engines": {
    "node": ">=12.0.0"
  }
}
