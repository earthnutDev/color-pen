{
  "name": "color-pen",
  "type": "module",
  "private": true,
  "version": "0.1.2",
  "main": "cjs/index.cjs",
  "typings": "types/index.d.ts",
  "description": "一个用于终端的彩 🖊️",
  "scripts": {
    "b": "rollup --config rollup.config.js && tsc -p tsconfig.types.json",
    "clean:package": "node scripts/clean-package-json.js",
    "build": "jja rm dist && npm run b && npm run clean:package",
    "dev": "jja cls rm .earthnut && rollup --config rollup.eg.config.js && node .earthnut/index.mjs",
    "eslint": "jja cls && eslint src",
    "beautify": "npm run eslint && npm run prettier",
    "prettier": "jja cls && prettier . --write",
    "prepare": "husky",
    "test": "lint-staged && jest --coverage",
    "report": "cat ./coverage/lcov.info | coveralls",
    "coverage": "codecov",
    "preview": "node example/index.js",
    "push:version": "gvv",
    "push:submit": "gvv",
    "vjj": "vjj",
    "prepublishOnly": "pjj"
  },
  "files": [
    "mjs",
    "cjs",
    "types"
  ],
  "exports": {
    ".": {
      "import": {
        "default": "./mjs/index.mjs",
        "types": "./types/index.d.ts"
      },
      "require": {
        "default": "./cjs/index.cjs",
        "types": "./types/index.d.ts"
      }
    }
  },
  "keywords": [
    "color-pen"
  ],
  "license": "ISC",
  "homepage": "https://earthnut.dev/color-pen",
  "bugs": {
    "url": "https://github.com/earthnutDev/color-pen/issues",
    "email": "earthnut.dev@outlook.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/earthnutDev/color-pen.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "a-js-tools": ">=0.1.4 <1.0.0",
    "a-type-of-js": ">=0.2.0 <1.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.0",
    "a-node-tools": "^1.0.0",
    "codecov": "^3.8.2",
    "coveralls": "^3.1.1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-jsdoc": "^50.6.9",
    "globals": "^16.0.0",
    "gvv": "^0.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jja": "^1.0.3",
    "lint-staged": "^15.5.0",
    "pjj": "^0.1.4",
    "prettier": "^3.5.3",
    "rollup": "^4.39.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-copy": "^3.5.0",
    "ts-jest": "^29.3.1",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.1",
    "vjj": "^0.1.2"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,md,mdx,json}": [
      "eslint src --fix",
      "prettier --write"
    ]
  },
  "author": {
    "name": "earthnut",
    "email": "earthnut.dev@outlook.com",
    "url": "https://earthnut.dev"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Edge versions",
    "not Safari >= 0",
    "not IE >= 0",
    "not Firefox >= 0"
  ],
  "engines": {
    "node": ">=12.0.0"
  }
}
